    "@types/mapbox-gl": "2.7.5",
    "@types/mapbox__mapbox-gl-geocoder": "4.7.2",
    "@types/node": "17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "commitizen": "4.2.5",
